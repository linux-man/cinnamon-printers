{
"show-icon": {
    "type": "radiogroup",
    "default": "always",
    "description": "Show icon",
    "options": { "Always": "always", "When printers exist": "printers", "When printing": "jobs"},
    "tooltip": "When to show the icon"
  },
 "show-error" : {
    "type" : "checkbox",
    "default" : true,
    "description": "Printer errors",
    "tooltip": "Show printer error icon"
 },
 "show-jobs" : {
    "type" : "checkbox",
    "default" : true,
    "description": "Documents count",
    "tooltip": "Show documents count next to icon"
 },
 "job-number" : {
    "type" : "checkbox",
    "default" : false,
    "description": "Document number",
    "tooltip": "Show each document number on print list"
 },
 "send-to-front" : {
    "type" : "checkbox",
    "default" : false,
    "description": "Send to front",
    "tooltip": "Documents can be sent to front"
 },
 "symbolic-icons" : {
    "type" : "checkbox",
    "default" : true,
    "description": "Symbolic icons",
    "tooltip": "Use symbolic icons"
 },
  "reload-button" : {
    "type" : "button",
    "description": "Reload Applet",
    "callback": "on_reload_button",
    "tooltip": "Reload Applet"
 },
   "cups-button" : {
    "type" : "button",
    "description": "Restart CUPS",
    "callback": "on_cups_button",
    "tooltip": "Restart CUPS"
 }
}
